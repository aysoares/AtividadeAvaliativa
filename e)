void main() {
  Questao q = new Questao(5,10,'Aylla');
  q.soma();
  q.nome();
  }
  
  class Questao {
    int a;
    int b;
    String c;
  
    Questao(this.a, this.b, this.c);
    
    void soma() {
      int soma;
      soma = a+b;
      print(soma);
    }
    
    void nome()  {
      print ('Meu nome Ã© $c');
    }
  }

